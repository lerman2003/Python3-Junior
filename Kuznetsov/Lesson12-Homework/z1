gos = {"Россия":"Моссква","франция":"Париж","США":"Вашингтон","Япония":"Токио","Украина":"Киев"}
r = input("Введите государтсво, чтобы получить название столицы: ")
for kl in gos:
    if kl==r:
        print(gos[kl])
        break
t = input("Введте столицу, чтобы получить название государства: ")
for el in gos:
    if t==gos[el]:
        print(el)
        break
