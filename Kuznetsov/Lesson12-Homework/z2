sl = {"1а":"19","1б":"18","2а":"21","2б":"20","3а":"17","3б":"16","4а":"23","4б":"20","5а":"24","5б":"19","6а":"22","6б":"14","7а":"23","7б":"21","8a":"19","8б":"20","9а":"22","9б":"16"}

kl = input("Введите класс, чтобы узнать колличество учеников в нем ")
for i in sl:
    if kl==i:
        print("В нём",sl[i],"человек")
