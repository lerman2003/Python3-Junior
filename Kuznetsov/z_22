a = int(input("Введите число a "))
b = int(input("Введите число b "))

print("===========")
if a!=0 and b!=0:
    if b%a==0:
        print("Число a являеться делителем b")
    if a%b==0:
        print("Число b являеться делителем a")
    else:
        print("Ни одно из чисел не являеться делителем")
else:
    print("Похоже у вас одно из чисел равно нулю")
