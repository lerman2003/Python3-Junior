n = int(input("Введите максимальный квадрат(Не больше 10000): "))
f = 2
while True:
    ch=f**2
    if ch>n and f<=100:
        print("---------------------")
        print("Квадрат числа:",f,"Больше значения:",n)
        break
    f+=1
    if f>100:
        print("---------------------")
        print("Вы ввели слишком большое значение n(по условию корень из числа не должен превышать значение 100)")
        break
