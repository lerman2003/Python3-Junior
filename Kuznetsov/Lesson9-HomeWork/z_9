from random import randint
import time
print("Подождите")
for i in range(4):
    time.sleep(0.5)
    print(".",end="")
print()
z = 0
Max=0
for i in range(1,13):
    a = randint(160,320)
    if a>Max:
        Max=a
        z=i
print("Самая большая скорость у:",z,"автомобиля, она достикает до:",Max,"km/ch")
