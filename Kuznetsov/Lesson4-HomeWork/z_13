n = int(input("Введите трехзначное число "))

print("---------------")
if 99<n<1000:
    n1 = n//100
    n2 = n%100//10
    n3 = n%100%10
    if n1==6 or n2==6 or n3==6:
        input("в него входит число 6")
    else:
        input("в него не входит число 6")
else:
    input("число не трехзначное")
